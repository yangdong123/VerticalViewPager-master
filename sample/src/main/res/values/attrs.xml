<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LinearGradientView">
        <attr name="radius_left_top" format="dimension"/>
        <attr name="radius_right_top" format="dimension"/>
        <attr name="radius_left_bottom" format="dimension"/>
        <attr name="radius_right_bottom" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CustomDraweeView">
        <attr name="default_icon_id" format="reference"/>
        <attr name="default_error_id" format="integer"/>
        <attr name="my_ratio" format="float"/>
    </declare-styleable>

    <!--文字描边-->
    <declare-styleable name="StrokeTextView">
        <attr name="outerColor" format="color|reference"/>
        <attr name="innnerColor" format="color|reference"/>
    </declare-styleable>
    <declare-styleable name="ActivityTitleBar">
        <attr name="activity_logo" format="reference"/>
        <attr name="activity_title" format="string"/>
        <attr name="activity_back" format="boolean"/>
        <attr name="activity_title_gravity">
            <flag name="center" value="0"/>
            <flag name="left" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="YViewPager">
        <attr name="orientation">
            <enum name="vertical" value="0"/>
            <enum name="horizontal" value="1"/>
        </attr>
        <attr name="circulatory" format="boolean"/>
        <attr name="circulationCount" format="integer"/>

    </declare-styleable>
    <declare-styleable name="MKLoader">
        <attr name="mk_color" format="color"/>
        <attr name="mk_type">
            <enum name="Sharingan" value="11"/>
            <enum name="TwinFishesSpinner" value="7"/>
            <enum name="ClassicSpinner" value="0"/>

            <enum name="LineSpinner" value="2"/>
            <enum name="FishSpinner" value="1"/>
            <enum name="PhoneWave" value="10"/>

            <enum name="ThreePulse" value="3"/>
            <enum name="FourPulse" value="4"/>
            <enum name="FivePulse" value="5"/>

            <enum name="Worm" value="8"/>
            <enum name="Whirlpool" value="9"/>
            <enum name="Radar" value="6"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="TitleBarView">
        <attr name="titleText" format="reference"/>
        <attr name="buttonText" format="reference"/>
        <attr name="buttonEnable" format="boolean"/>
        <attr name="buttonColor" format="reference"/>
        <attr name="buttonVisibility" format="boolean"/>
        <attr name="iconDrawable" format="reference"/>
        <attr name="iconEnable" format="boolean"/>
        <attr name="iconVisibility" format="boolean"/>
        <attr name="backVisibility" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ShimmerFrameLayout">
        <attr name="auto_start" format="boolean"/>
        <attr name="base_alpha" format="float"/>
        <attr name="duration" format="integer"/>
        <attr name="repeat_count" format="integer"/>
        <attr name="repeat_delay" format="integer"/>
        <attr name="repeat_mode" format="enum">
            <enum name="restart" value="1"/>
            <enum name="reverse" value="2"/>
        </attr>
        <attr name="angle" format="enum">
            <enum name="cw_0" value="0"/>
            <enum name="cw_90" value="90"/>
            <enum name="cw_180" value="180"/>
            <enum name="cw_270" value="270"/>
        </attr>
        <attr name="dropoff" format="float"/>
        <attr name="fixed_width" format="dimension"/>
        <attr name="fixed_height" format="dimension"/>
        <attr name="intensity" format="float"/>
        <attr name="relative_width" format="float"/>
        <attr name="relative_height" format="float"/>
        <attr name="shape" format="enum">
            <enum name="linear" value="0"/>
            <enum name="radial" value="1"/>
        </attr>
        <attr name="tilt" format="float"/>
    </declare-styleable>


    <declare-styleable name="UltraViewPager">
        <attr name="upv_autoscroll" format="integer"/>
        <attr name="upv_infiniteloop" format="boolean"/>
        <attr name="upv_ratio" format="float"/>
        <attr name="upv_scrollmode" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="upv_disablescroll" format="enum">
            <enum name="none" value="0"/>
            <enum name="backward" value="1"/>
            <enum name="forward" value="2"/>
        </attr>
        <attr name="upv_multiscreen" format="boolean"/>
        <attr name="upv_automeasure" format="boolean"/>
        <attr name="upv_itemratio" format="float"/>
    </declare-styleable>


</resources>